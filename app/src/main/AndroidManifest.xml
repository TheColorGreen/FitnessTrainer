<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fitness.sportgenertaion.fitnesstrainer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CrearRutina"
            android:label="@string/title_activity_crear_rutina"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".VerEjercicio"
            android:label="@string/title_activity_ver_ejercicio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Dias"
            android:label="@string/title_activity_dias"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CrearRutinas"
            android:label="@string/title_activity_crear_rutinas"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>